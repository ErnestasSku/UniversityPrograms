<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">


    <xs:element name="games">
        <xs:unique name="uniqueTitle">
            <xs:selector xpath="game"/>
            <xs:field xpath="name/text"/>
        </xs:unique>
    </xs:element>

    <xs:element name="download-pages">
        <xs:unique name="uniqueDownloads">
            <xs:selector xpath="download-pages"/>
            <xs:field xpath="download-page/text"></xs:field>
        </xs:unique>
    </xs:element>

</xs:schema>